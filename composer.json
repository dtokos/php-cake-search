{
    "name": "artvys/cake-search",
    "description": "CakePHP adapter for artvys/search package",
    "type": "library",
    "require": {
        "artvys/search": "^1.0",
        "cakephp/cakephp": "4.4.*",
        "php": "^8.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1",
        "phpstan/phpstan": "^1.10"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Artvys\\Search\\Cake\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
